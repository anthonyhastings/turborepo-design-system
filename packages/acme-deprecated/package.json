{
  "name": "@acme/deprecated",
  "version": "0.0.0",
  "description": "Collection of deprecated components.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react",
    "clean": "rm -rf .turbo && rm -rf cypress/screenshots && rm -rf cypress/videos && rm -rf node_modules && rm -rf dist",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "test": "cypress run --component",
    "test:gui": "cypress open --component --browser chrome",
    "types:check": "tsc --noEmit"
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.3",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@vitejs/plugin-react": "^2.0.1",
    "config-prettier": "0.0.0",
    "config-tsconfig": "0.0.0",
    "cypress": "^10.5.0",
    "eslint": "^8.15.0",
    "eslint-config-acme": "0.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "tsup": "^5.10.1",
    "typescript": "^4.5.4",
    "vite": "^3.0.8"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
